@using MaintenanceSchedule.Library.Utilities

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html lang="vi">
<head>
    <title>@ViewBag.Title</title>
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <meta http-equiv="X-UA-Compatible" content="IE=9" />
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="Expires" content="-1" />
    <meta http-equiv="Content-Style-Type" content="text/css" />
    <meta http-equiv="Content-Script-Type" content="text/javascript" />

    <link rel="SHORTCUT ICON" href="http://cdn.rcstatic.com/images/rclogo/blue/logomark-rclogo_32x32.png" type="image/x-icon" />
    <link href="https://plus.google.com/103918092640896887346" rel="publisher" />

    <meta property="fb:app_id" content="320721568258779" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="@ViewBag.FacebookUrl" />
    <meta property="og:title" content="@Characters.GetStringWithoutHttp(ViewBag.FacebookUrl)" />
    <meta property="og:description" content="Đặt lịch bảo dưỡng, bảo dưỡng định kỳ ô tô, chi phí bảo dưỡng định kỳ ôtô,
                            bảng giá bảo dưỡng định kỳ xe hơi, dịch vụ bảo dưỡng ôtô! – datlichbaoduong.com" />
    <meta property="og:image" content="http://datlichbaoduong.com/Assets/images/logo-medium.png" />
    <meta property="og:image:type" content="image/png" />
    <meta property="og:image:width" content="600" />
    <meta property="og:image:height" content="315" />
    <meta name="viewport" content="initial-scale = 1,user-scalable=no,maximum-scale=1.0" />

    <!-- Css -->
    @Styles.Render("~/Css/Layout")
    @Styles.Render("~/Css/Home/Form")
    @*@Styles.Render("~/Css/Alert")*@

    <!-- Scripts -->
    @Scripts.Render("~/Js/Jquery")
    @Scripts.Render("~/Js/JqueryValidation")
    @Scripts.Render("~/Js/Alert")
    <script src="https://apis.google.com/js/platform.js"></script>
</head>

<body class="home" data-page-type="HomePage">
    @{ Html.RenderPartial("_Header"); }
    <!-- End of header -->

    @{ Html.RenderPartial("_Form"); }

    @RenderBody()
    <!-- End of body -->

    <div id="fb-root"></div>
    <script>
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v2.6&appId=320721568258779";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script>

    @{ Html.RenderPartial("_Facebook"); }
    <!-- End of facebook -->

    <iframe id="footerFrame" src="@Url.Action("Footer", "Home", new { area = "Common" })" style="height: 447px; width: 100%;"></iframe>
</body>
</html>
